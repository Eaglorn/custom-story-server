{
  "name": "custom-story-server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "set NODE_ENV=dev && node ./index.js",
    "prod": "set NODE_ENV=prod && node ./index.js",
    "generate": "npx prisma generate",
    "migrate:postgresql": "npx prisma migrate dev --schema prisma/postgresql/schema.prisma && yarn generate --schema prisma/postgresql/schema.prisma",
    "migrate:mongodb": "npx prisma migrate dev --schema prisma/mongodb/schema.prisma && yarn generate --schema prisma/mongodb/schema.prisma",
    "studio": "npx prisma studio",
    "docs": "npx prisma-docs-generator serve",
    "up": "yarn install && yarn upgrade"
  },
  "dependencies": {
    "@prisma/client": "^5.14.0",
    "async": "^3.2.5",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "decimal.js": "^10.4.3",
    "express": "^4.19.2",
    "luxon": "^3.4.4",
    "md5": "^2.3.0",
    "mongodb": "^6.6.2",
    "node-schedule": "^2.1.1",
    "nodemailer": "^6.9.13",
    "pg": "^8.11.5",
    "radash": "^12.1.0",
    "socket.io": "^4.7.5",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "eiows": "^7.0.3"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "prisma": "^5.14.0"
  }
}
