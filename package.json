{
  "name": "custom-story-server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "set NODE_ENV=dev && node ./index.js --env-file=.env",
    "prod": "set NODE_ENV=prod && node ./index.js --env-file=.env",
    "generate": "npx prisma generate --schema prisma/postgresql/schema.prisma && npx prisma generate --schema prisma/mongodb/schema.prisma",
    "generate:postgresql": "npx prisma generate --schema prisma/postgresql/schema.prisma",
    "generate:mongodb": "npx prisma generate --schema prisma/mongodb/schema.prisma",
    "migrate": "npx prisma migrate dev --schema prisma/postgresql/schema.prisma && yarn generate:postgresql",
    "studio": "npx prisma studio",
    "docs": "npx prisma-docs-generator serve",
    "up": "yarn install && yarn upgrade",
    "pm2:start": "pm2 start ecosystem.config.js",
    "logs": "pm2 logs",
    "monitor": "pm2 monit",
    "kill": "pm2 kill"
  },
  "dependencies": {
    "@prisma/client": "^5.15.0",
    "@socket.io/redis-streams-adapter": "^0.2.2",
    "async": "^3.2.5",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "decimal.js": "^10.4.3",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "luxon": "^3.4.4",
    "md5": "^2.3.0",
    "mongodb": "^6.7.0",
    "node-schedule": "^2.1.1",
    "nodemailer": "^6.9.13",
    "pg": "^8.12.0",
    "radash": "^12.1.0",
    "socket.io": "^4.7.5",
    "uuid": "^9.0.1",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "prettier": "^3.3.1",
    "prisma": "^5.15.0"
  }
}
